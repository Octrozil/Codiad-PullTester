[ { "author" : "daeks",
    "version": "0.4",
    "name" : "Codiad PullTester",
    "url" : "https://github.com/daeks/Codiad-PullTester.git",
    "rightbar" : [{
        "action" : "codiad.tester.open();",
        "icon" : "icon-network",
        "title" : "Codiad Requests"
        }],
     "contextmenu" : [{
        "action" : "codiad.filemanager.openInBrowser('/'+$('#context-menu').attr('data-path'));",
        "icon" : "icon-network",
        "applies-to" : "directory-only no-external",
        "title" : "Open Request"
        }]
  } ]